>> a=poly([1/0.5]);
>> b=poly([0.5]);
>> zplane(a,b);
>> freqz(a,b);


%% overeni zda je H=(0.6+z^-1)/(1+0.6*z^-1) fazovaci clanek

>> b=poly([0.6]);
>> a=poly([1/0.6]);
>> zplane(a,b);
>> freqz(a,b);

%% nebo by Misurec

>> cit=[0.6 1];
>> jm=[1 0.6];
>> zplane(cit,jm)
>> freqz(cit,jm)


>> r=0.9;
>> w=pi/4;
>> cit=[r^2 -2*r*cos(w) 1];
>> jm=[1 -2*r*cos(w) r^2];
>> zplane(cit,jm)
>> freqz(cit,jm)
